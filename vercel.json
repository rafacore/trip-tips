{
  "version": 2,
  "builds": [
    {
      "src": "client/**/*",
      "use": "@vercel/static-build",
      "config": { "distDir": "client/build" }
    },
    {
      "src": "index/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/index/(.*)",
      "dest": "/index/index/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}